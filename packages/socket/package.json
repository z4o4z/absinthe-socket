{
  "name": "@absinthe/socket",
  "version": "0.2.1",
  "description": "Absinthe Socket",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:esm": "tsc --project ./tsconfig.lib.json --module esnext --outDir ./dist/esm",
    "build:cjs": "tsc --project ./tsconfig.lib.json --module commonjs --outDir ./dist/cjs",
    "clean": "rimraf build",
    "lint": "eslint '**/*.ts'",
    "lint:output": "yarn run eslint-output --quiet \"**/*.ts\"",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint:output",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/absinthe-graphql/absinthe-socket.git"
  },
  "author": "Mauro Titimoli <mgtitimoli@gmail.com> (https://github.com/mgtitimoli)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/absinthe-graphql/absinthe-socket/issues"
  },
  "homepage": "https://github.com/absinthe-graphql/absinthe-socket#readme",
  "keywords": [
    "absinthe",
    "phoenix",
    "socket",
    "client",
    "graphql",
    "utilities",
    "utils"
  ],
  "dependencies": {
    "@types/phoenix": "^1.5.4",
    "@absinthe/graphql-utils": "^0.2.1",
    "fast-deep-equal": "^3.1.3",
    "zen-observable-ts": "^1.1.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "phoenix": "1.6.6"
  }
}
