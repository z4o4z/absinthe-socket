{
  "name": "@absinthe/socket-relay",
  "version": "0.2.1",
  "description": "Absinthe Socket Relay",
  "module": "dist/index.js",
  "main": "src/index.js",
  "react-native": "src/index",
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:esm": "tsc --project ./tsconfig.lib.json --module esnext --outDir ./dist/esm",
    "build:cjs": "tsc --project ./tsconfig.lib.json --module commonjs --outDir ./dist/cjs",
    "clean": "rimraf build",
    "lint": "eslint '**/*.ts'",
    "lint:output": "yarn run eslint-output --quiet \"**/*.ts\"",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint:output",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/absinthe-graphql/absinthe-socket.git"
  },
  "author": "Mauro Titimoli <mgtitimoli@gmail.com> (https://github.com/mgtitimoli)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/absinthe-graphql/absinthe-socket/issues"
  },
  "homepage": "https://github.com/absinthe-graphql/absinthe-socket/tree/master/packages/absinthe-socket-relay#readme",
  "keywords": [
    "absinthe",
    "phoenix",
    "socket",
    "relay",
    "client",
    "graphql",
    "utilities",
    "utils"
  ],
  "dependencies": {
    "@absinthe/graphql-utils": "^0.2.1",
    "@absinthe/socket": "0.2.1",
    "react-relay": "13.2.0",
    "relay-runtime": "^13.2.0"
  },
  "devDependencies": {
    "@types/react-relay": "^13.0.1"
  }
}
